<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/roolasdas"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@color/default_screen_bg"
	android:orientation="vertical" >

	<LinearLayout
		android:id="@+id/linearLayout1"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_centerVertical="true"
		android:layout_toRightOf="@+id/llCenter"
		android:orientation="vertical" >

		<TextView
			android:id="@+id/tw3"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:background="@drawable/circle"
			android:gravity="center"
			android:lines="1"
			android:maxLines="1"
			android:singleLine="true"
			android:text="3"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:textColor="@android:color/white" />

		<TextView
			android:id="@+id/tw6"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:background="@drawable/circle"
			android:gravity="center"
			android:lines="1"
			android:maxLines="1"
			android:singleLine="true"
			android:text="6"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:textColor="@android:color/white"
			android:textStyle="bold" />

		<TextView
			android:id="@+id/tw9"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:background="@drawable/circle"
			android:gravity="center"
			android:lines="1"
			android:maxLines="1"
			android:singleLine="true"
			android:text="9"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:textColor="@android:color/white"
			android:textStyle="bold" />

		<TextView
			android:id="@+id/twLogin"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:background="@drawable/circle"
			android:gravity="center"
			android:lines="1"
			android:maxLines="1"
			android:singleLine="true"
			android:text="Login"
			android:textAppearance="?android:attr/textAppearanceSmall"
			android:textColor="@android:color/white"
			android:textStyle="bold" />
	</LinearLayout>

	<LinearLayout
		android:id="@+id/llCenter"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_centerInParent="true"
		android:orientation="vertical" >

		<TextView
			android:id="@+id/tw2"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:background="@drawable/circle"
			android:gravity="center"
			android:lines="1"
			android:maxLines="1"
			android:singleLine="true"
			android:text="2"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:textColor="@android:color/white"
			android:textStyle="bold" />

		<TextView
			android:id="@+id/tw5"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:background="@drawable/circle"
			android:gravity="center"
			android:lines="1"
			android:maxLines="1"
			android:singleLine="true"
			android:text="5"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:textColor="@android:color/white"
			android:textStyle="bold" />

		<TextView
			android:id="@+id/tw8"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:background="@drawable/circle"
			android:gravity="center"
			android:lines="1"
			android:maxLines="1"
			android:singleLine="true"
			android:text="8"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:textColor="@android:color/white"
			android:textStyle="bold" />

		<TextView
			android:id="@+id/tw0"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:background="@drawable/circle"
			android:gravity="center"
			android:lines="1"
			android:maxLines="1"
			android:singleLine="true"
			android:text="0"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:textColor="@android:color/white"
			android:textStyle="bold" />
	</LinearLayout>

	<LinearLayout
		android:id="@+id/linearLayout2"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_centerVertical="true"
		android:layout_toLeftOf="@+id/llCenter"
		android:orientation="vertical" >

		<TextView
			android:id="@+id/tw1"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_centerInParent="true"
			android:background="@drawable/circle"
			android:gravity="center"
			android:lines="1"
			android:maxLines="1"
			android:singleLine="true"
			android:text="1"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:textColor="@android:color/white"
			android:textStyle="bold" />

		<TextView
			android:id="@+id/tw4"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_centerInParent="true"
			android:background="@drawable/circle"
			android:gravity="center"
			android:lines="1"
			android:maxLines="1"
			android:singleLine="true"
			android:text="4"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:textColor="@android:color/white"
			android:textStyle="bold" />

		<TextView
			android:id="@+id/tw7"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_centerInParent="true"
			android:background="@drawable/circle"
			android:gravity="center"
			android:lines="1"
			android:maxLines="1"
			android:singleLine="true"
			android:text="7"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:textColor="@android:color/white"
			android:textStyle="bold" />

		<TextView
			android:id="@+id/twc"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_centerInParent="true"
			android:background="@drawable/circle"
			android:gravity="center"
			android:lines="1"
			android:maxLines="1"
			android:singleLine="true"
			android:text="C"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:textColor="@android:color/white"
			android:textStyle="bold" />
	</LinearLayout>

	<EditText
		android:id="@+id/etPassword"
		android:layout_width="wrap_content"
		android:layout_height="50dp"
		android:background="@drawable/transparent_inside_and_white_border"
		android:padding="3dp"
		android:layout_above="@+id/linearLayout1"
		android:layout_alignLeft="@+id/linearLayout2"
		android:layout_alignRight="@+id/linearLayout1"
		android:ems="10"
		android:inputType="numberPassword" >

		<requestFocus />
	</EditText>

</RelativeLayout>